{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "composite": true,
    "declaration": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ES2020"],
    "typeRoots": [
      "../../../../../../bzl/valdi/npm/node_modules/@types"
    ],
    "paths": {
      "@protobuf-ts/runtime": ["../../../../../../bzl/valdi/npm/node_modules/@protobuf-ts/runtime"]
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts"
  ],
  "exclude": [
    "**/*.js",
    "**/node_modules/**",
    "node_modules",
    "src_symlink"
  ]
}